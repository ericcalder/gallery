<script>


  
        
      (function() {
        'use strict';
var items=JSON.parse(localStorage["det"])

$('h4.d-flex.justify-content-between.align-items-center.mb-3   span.badge.badge-secondary.badge-pill').append(items.length)

cartItem(items)

        window.addEventListener('load', function() {
              var name=JSON.parse(window.localStorage.getItem('det')).name;
              var cost=JSON.parse(window.localStorage.getItem('det')).cost;
              $('.my-0:eq(0)').append(name)
              $('span.text-muted:eq(1)').append('£'+cost)
          // Fetch all the forms we want to apply custom Bootstrap validation styles to
          var forms = document.getElementsByClassName('needs-validation');
console.log('forms[0][0].value=='+forms[0][0].value)
console.log('forms[0][1].value=='+forms[0][1].value)
console.log('forms[0].className=='+forms[0].className)
          // Loop over them and prevent submission
          var validation = Array.prototype.filter.call(forms, function(form) {
            form.addEventListener('submit', function(event) {
              console.log('form======'+form[1].value)
                    if (form.checkValidity() === false) {
                      event.preventDefault();
                      event.stopPropagation();
                    }//if
              form.classList.add('was-validated');


            }, false);//addEventListener('submit', function, capture=false)
         
          });//validation
          
        }, false);//window.addEventListener('load', function()
      })();//(function() {'use strict
 

///////////////functions////////////
function cartItem(items){
  for(var i=0;i<items.length;i++){
var li=$('<li />',{class:'list-group-item', text:'List item'})
var div=$('<div />')
var product=$('<h6 />',{class:'my-0', text:items[i].name})
var image=$('<img />',{src: items[i].src, width:'50'})
var cost=$('<h4 />',{class:'float-right',text:'£'+items[i].cost})

$('div.col-md-4 ul.list-group.mb-3').append(li
                    .append(div
                      .append(product, image),cost))  
}//for
}

//////////////////////////////



    </script>

